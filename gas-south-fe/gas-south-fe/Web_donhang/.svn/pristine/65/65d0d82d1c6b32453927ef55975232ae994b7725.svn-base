                }
                
                /*
                Label the data
                */
                td:nth-of-type(1):before { content: "Nhập hàng từ"; }
                td:nth-of-type(2):before { content: "Ngày giờ"; }
                td:nth-of-type(3):before { content: "Số lượng"; }
              
            }
            .R{
