#detail-export-order{
    th{
        background-color: #009347 !important;
        color: #FFF;
        text-align: center;
    }
    td{
        text-align: center;
    }
    .statuss{
        a{
            padding: 0;
            margin: 0px 10px;
            font-size: 1.1rem;
        }
    }
    .react-datepicker-wrapper{
        display: block;
    }
    .react-datepicker__input-container{
        display: flex;
        input{
            border: 1px #d9d9d9 solid;
            border-radius: 4px;
            width: 369px;
            padding: 4px 11px;
        }
    }
    .edit-export-manager{
        background-color: #009347 !important;
        color: #FFF;
    }
    .form-border{
        border: 1px #e7e7e7 solid;
        border-radius: 10px;
        padding: 10px;
        margin: 10px 0;
    }
    @media (max-width: 1200px) {
        .ant-table {
          width: unset;
        }
      }

      
}
.ant-notification.ant-notification-topRight{
    z-index: 20000;
}