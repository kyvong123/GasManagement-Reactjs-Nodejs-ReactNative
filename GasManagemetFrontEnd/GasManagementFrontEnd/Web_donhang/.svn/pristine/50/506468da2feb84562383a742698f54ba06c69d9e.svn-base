#usermanuall {
  .card {
    .card-title a {
      color: #fff;
      font-size: 14px;
      text-decoration: none;

      &:hover {
        background: none;
        color: #fff;
      }
    }

    .card-view {
      height: 650px;
      overflow: hidden;

      .text-center {
        height: 550px;
        margin-top: 15px;
        overflow: hidden;
        box-shadow: 0 15px 20px rgba(0, 0, 0, 0.3);

        .is-gapless {
          position: absolute;
          top: 10;
          display: flex;
          flex-wrap: wrap;
          top: 94%;
          left: 34%;

          .is-2 {
            display: none;
          }

          .is-3 {
            display: flex;

            button {
              width: 170px;
              border-radius: 40px;
              background-color: blue;
              color: wheat;
              text-align: center;
              align-items: center;
              cursor: pointer;

              .is-small {
                position: relative;
                top: 3px;
              }

              &:focus {
                outline: none;
              }

              &:disabled {
                cursor: no-drop;
              }
            }

            .is-size-7 {
              padding: 7px 0;
              text-align: center;
              align-items: center;
            }
          }
        }
      }
    }

    .dd {
      h2 {
        color: #009347;
        font-weight: 700;
      }

      .d {
        margin: 10px 0;

        h4 {
          color: #009347;
          font-weight: 700;
        }

        .btn-create {
          background-color: #009347;

          a {
            color: #fff;
            text-decoration: none;

            &:hover {
              color: #fff;
              background: none;
            }

            &:active {
              color: #fff;
              text-decoration: none;
            }
          }
        }

        .n {
          background-color: orange;
        }
      }
    }
  }

  @media screen and (min-width: 992px) {
    .dd {
      h2 {
        padding-left: 17px;
      }

      .d {
        h4 {
          padding-left: 22px;
        }

        .btn-create {
          margin-top: 10px;
          width: 50%;
        }
      }
    }
  }

  @media screen and (max-width: 992px) {
    .card {
      .dd {
        h2 {
          padding-left: 17px;
        }

        .d {
          h4 {
            padding-left: 22px;
          }

          .btn-create {
            margin-top: 10px;
            width: 100%;
          }
        }
      }
    }
  }

  @media screen and (max-width: 768px) {
    .card {
      .card-title {
        h4 {
          font-size: 17px;
        }

        button {
          padding: 0;
          margin-left: 0;

          a {
            font-size: 12px;
          }
        }
      }

      .card-view {
        height: 485px;
        margin-top: 5px;

        .text-center {
          canvas {
            max-width: 100%;
          }

          .is-gapless {
            top: 92%;
            left: 24%;

            .is-3 {
              button {
                .is-small i {
                  font-size: 20px;
                }

                .is-size-7 {
                  width: 90px;
                }
              }
            }
          }
        }
      }

      .dd {
        h2 {
          padding-left: 17px;
        }

        .d {
          h4 {
            padding-left: 22px;
          }

          .btn-create {
            margin-top: 10px;
            width: 100%;
          }
        }
      }
    }
  }
}

#frequently {
  .card {
    .card-title {
      .flexbox {
        padding-left: 0;
        padding-top: 0;
      }
    }
  }

  .menu1 {
    font-size: 17px;
    font-weight: 700;
    font-family: "Roboto";
    list-style: none;
    padding-left: 0;
    margin-bottom: 30px;
  }
}

#create-support {
  .modal-content {
    .modal-body {
      .card {
        .card-body {
          .image {
            width: 210px;
            margin: 5px 0;
          }

          #nut {
            border: 0;
            width: 40px;
            background-color: orange;
            border-radius: 24%;
            float: left;
            margin-right: 10px;
            color: wheat;
            text-align: center;
            align-items: center;
          }
        }
      }
    }
  }
}

#update-support {
  .modal-content {
    .modal-body {
      .card {
        .card-body {
          .form-group {
            .custom-file-label {
              top: 125px;
              left: 13px;
              width: 279px;
              font-size: 14px;
            }
          }

          .image {
            width: 210px;
            margin: 5px 0;
          }

          #nut {
            border: 0;
            width: 40px;
            background-color: orange;
            border-radius: 24%;
            float: left;
            margin-right: 10px;
            color: wheat;
            text-align: center;
            align-items: center;
          }
        }
      }
    }
  }
}

#supportmanagement {
  table {
    tbody {
      tr {
        td {
          font-size: 17px;
          font-weight: 700;
        }
      }
    }
  }
  .colap {
    position: relative;
    p {
      margin-bottom: 4px;
      font-size: 16px;

      a {
        color: black !important;
      }

      .delete {
        position: absolute;
        right: 5px;
        text-decoration: none;

        &:hover {
          background: none;
        }
      }
    }

    .collapse {
      top: -3px;
      position: relative;
      font-size: 15px;
      font-weight: normal;
      .text-content {
        margin-bottom: 0;

        .img {
          width: 250px;
          overflow: hidden;

          img {
            max-width: 100%;
            width: 100%;
          }
        }
      }
    }
  }
}
